{
    "name": "restruct/silverstripe-mfa-bundle",
    "description": "Portable Silverstripe MFA bundle with TOTP and WebAuthn support, plus configurable TOTP settings",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "mfa",
        "totp",
        "webauthn",
        "2fa",
        "authentication"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Restruct",
            "email": "dev@restruct.nl",
            "homepage": "https://restruct.nl"
        }
    ],
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/mfa": "^5.0",
        "silverstripe/totp-authenticator": "^5.0",
        "silverstripe/webauthn-authenticator": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Restruct\\MFABundle\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    }
}